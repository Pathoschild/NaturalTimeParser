    <projectUrl>https://github.com/Pathoschild/NaturalTimeParser#readme</projectUrl>
    <iconUrl>https://upload.wikimedia.org/wikipedia/commons/thumb/d/d0/Noun_project_3067.svg/128px-Noun_project_3067.svg.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>datetime string.format</tags>
    <dependencies>
      <dependency id="Pathoschild.NaturalTimeParser" />
      <dependency id="SmartFormat.NET" />
    </dependencies>
  </metadata>
</package>